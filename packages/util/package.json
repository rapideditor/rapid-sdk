{
  "name": "@rapid-sdk/util",
  "version": "1.0.0-pre.3",
  "description": "Collection of rapid-sdk utility libraries",
  "homepage": "https://github.com/rapideditor/rapid-sdk/tree/main/packages/util#readme",
  "repository": "github:rapideditor/rapid-sdk",
  "license": "ISC",
  "contributors": [
    "Ben Clark <clarkben@fb.com> (https://github.com/bonkles)",
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)",
    "Danny Krause (https://github.com/vanreece)",
    "Milos Brzakovic <mbrzakovic@microsoft.com> (https://github.com/mbrzakovic)"
  ],
  "files": [
    "dist/",
    "src/"
  ],
  "type": "module",
  "typedocMain": "./src/util.ts",
  "source": "./src/util.ts",
  "exports": {
    "types": "./dist/util.d.ts",
    "import": "./dist/util.mjs",
    "require": "./dist/util.cjs"
  },
  "scripts": {
    "all": "run-s clean build lint test",
    "build": "run-p build:js build:types",
    "build:js": "bun ./scripts/build.ts",
    "build:types": "tsc",
    "clean": "bun ./scripts/clean.ts",
    "lint": "eslint .",
    "test": "bun test --dots --coverage ./test"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "diacritics": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
