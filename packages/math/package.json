{
  "name": "@rapid-sdk/math",
  "version": "1.0.0-pre.4",
  "description": "Collection of rapid-sdk math libraries",
  "homepage": "https://github.com/rapideditor/rapid-sdk/tree/main/packages/math#readme",
  "repository": "github:rapideditor/rapid-sdk",
  "license": "ISC",
  "contributors": [
    "Ben Clark <clarkben@fb.com> (https://github.com/bonkles)",
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)",
    "Danny Krause (https://github.com/vanreece)",
    "Milos Brzakovic <mbrzakovic@microsoft.com> (https://github.com/mbrzakovic)"
  ],
  "files": [
    "dist/",
    "src/"
  ],
  "type": "module",
  "typedocMain": "./src/math.ts",
  "source": "./src/math.ts",
  "exports": {
    "bun": "./src/math.ts",
    "types": "./dist/math.d.ts",
    "import": "./dist/math.mjs",
    "require": "./dist/math.cjs"
  },
  "scripts": {
    "all": "run-s clean build lint test",
    "build": "run-p build:js build:ts",
    "build:js": "bun ./scripts/build.ts",
    "build:ts": "tsc",
    "clean": "bun ./scripts/clean.ts",
    "lint": "eslint .",
    "test": "bun test --dots --coverage ./test"
  },
  "dependencies": {
    "@types/d3-polygon": "^3.0.2",
    "d3-polygon": "^3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
